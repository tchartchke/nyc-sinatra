<h2>Edit Historical Landmark</h2>

<form method="post" action="/landmarks/<%=@landmark.id%>">
  <input id="hidden" type="hidden" name="_method" value="patch">
  Name: <input type="text" value="<%=@landmark.name%>" name="landmark[name]" id="name">
  Year Completed: <input type="text" value="<%=@landmark.year_completed%>" name="landmark[year_completed]" id="year_completed">
  <input type="submit" value="Edit Landmark">
</form>

<a class="button" href="/landmarks">Landmarks</a>
<a class="button" href="/">Home</a>



